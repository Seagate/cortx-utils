{
  "install_path": "/opt/seagate",
  "discovery": {
    "health_provider": {
      "storage": "/opt/seagate/cortx/sspl/bin/resource_map",
      "server": "/opt/seagate/cortx/sspl/bin/resource_map"
    },
    "resource_map": {
      "location": "/var/cortx/resources/",
      "store_type": "json",
      "requests_file": "/var/cortx/resources/requests.json",
      "expiry_sec": 1800
    }
  }
}
