[Unit]
Description=cortx rest server
After=network.target

[Service]
User=root
WorkingDirectory=/usr/lib/python3.6/site-packages/cortx/utils/rest_server
ExecStart=/usr/bin/python3 /usr/lib/python3.6/site-packages/cortx/utils/rest_server/message_bus_rest.py
Restart=always

[Install]
WantedBy=multi-user.target
